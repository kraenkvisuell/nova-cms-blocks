{
    "name": "kraenkvisuell/nova-cms-blocks",
    "description": "Blocks Content & Repeater Fields for Laravel Nova.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "flexible",
        "repeat",
        "group",
        "layout"
    ],
    "authors": [
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be"
        },
        {
            "name": "Sebastian Hilger",
            "email": "hilger@kraenk.de"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaCmsBlocks\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaCmsBlocks\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
